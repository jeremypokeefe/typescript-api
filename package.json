{
  "name": "typescript-base-api",
  "version": "1.0.0",
  "description": "A starter Typescript API project using GraphQL, InversifyJS, and Express.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "InversifyJS",
    "Typescript",
    "TypeOrm",
    "GraphQL"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint --project ./tsconfig.json",
    "dev": "NODE_ENV=development tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start": "node ./dist/server.js",
    "build": "rimraf dist && tsc",
    "build:docs": "apidoc -i src -o dist/apidocs/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Jeremy O'Keefe",
  "license": "ISC",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@types/nodemailer": "^6.4.4",
    "base64url": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphile-utils": "^4.12.1",
    "handlebars": "^4.7.7",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "knex-migrate": "^1.7.4",
    "mailgun.js": "^6.0.1",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.2",
    "papaparse": "^5.3.1",
    "pg": "^8.5.1",
    "postgraphile": "^4.12.3",
    "postgraphile-plugin-connection-filter": "^2.2.2",
    "puppeteer": "^14.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "typeorm": "^0.2.37",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "typescript": "^4.6.3",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.0.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mailgun-js": "^0.22.12",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "acorn": "^6.4.1",
    "apidoc": "^0.50.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "helmet": "^5.0.1",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "prettier": "^1.18.2",
    "sqlite3": "^5.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^27.1.3",
    "tsc-watch": "^5.0.3",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1"
  }
}
